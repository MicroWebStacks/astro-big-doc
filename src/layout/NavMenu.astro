---
import './colors.css';
import SideMenu from './SideMenu.astro';
import { process_pages_list } from "./side_menu";
import {getDocuments} from 'content-structure'

export interface Props {
	section: string;
}

const {section} = Astro.props;

const documents = await getDocuments()
const menu_items = process_pages_list(documents,section)
console.log("NavMenu>")
---
<SideMenu items={menu_items}/>
